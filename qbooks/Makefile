####### Install

install_data: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/elkirtasse/data/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/elkirtasse/data/ 
	-$(INSTALL_DIR) /home/ali/elkirtasse-group/elkirtasse_3.6/data/images $(INSTALL_ROOT)/usr/share/elkirtasse/data/


uninstall_data:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/elkirtasse/data/images
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/elkirtasse/data/ 


install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/elkirtasse/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/elkirtasse/ 
	-$(INSTALL_PROGRAM) "../$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/share/elkirtasse/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/usr/share/elkirtasse/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/share/elkirtasse/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/elkirtasse/ 


install_icon: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ 
	-$(INSTALL_FILE) /home/ali/elkirtasse-group/elkirtasse_3.6/qbooks/usr/share/icons/hicolor/scalable/apps/elkirtasse.png $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/


uninstall_icon:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/elkirtasse.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ 


install_icon48: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ 
	-$(INSTALL_FILE) /home/ali/elkirtasse-group/elkirtasse_3.6/qbooks/usr/share/icons/hicolor/48x48/apps/elkirtasse.png $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/


uninstall_icon48:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/elkirtasse.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ 


install_applications: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/applications/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/applications/ 
	-$(INSTALL_PROGRAM) /home/ali/elkirtasse-group/elkirtasse_3.6/qbooks/usr/share/applications/elkirtasse.desktop $(INSTALL_ROOT)/usr/share/applications/
	-strip $(INSTALL_ROOT)/usr/share/applications/elkirtasse.desktop


uninstall_applications:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/applications/elkirtasse.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/applications/ 


